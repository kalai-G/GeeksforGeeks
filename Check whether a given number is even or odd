class Main {
    public static void main(String[] args) {
        int i=128;
        if(evenOdd(i)== true)
        {
            System.out.println("Given number is an even number");
        }
        else{
             System.out.println("Given number is an Odd number");
        }
       
        
        
    }
    public static boolean evenOdd(int n)
    {
        if(n%2 ==0)
        {
            return true;
        }
        return false;
    }
}
